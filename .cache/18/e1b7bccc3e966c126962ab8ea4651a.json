r (var i = 1; i<=100; i++){\r\n    const blocks = document.createElement('div')\r\n    blocks.classList.add('block');\r\n    leyenda1.appendChild(blocks)\r\n}\r\nfunction generate(){\r\n    anime({\r\n        targets: '.block',\r\n        translateX: function(){\r\n            return anime.random(-1000,2000)\r\n        },\r\n        translateY: function(){\r\n            return anime.random(-700,2000)\r\n        },\r\n        scale: function(){\r\n            return anime.random(1,5)\r\n        }\r\n    })\r\n    return(false)\r\n}\r\n\r\n\r\n//Animación escritura\r\n\r\nconst typed = new Typed ('.typed',{\r\n    strings:[\r\n        'Fullstack developer in process', \r\n        'Based in Argentina'],\r\n    typeSpeed:75,\r\n    startDelay:300,\r\n    backspeed:75,\r\n    smartBackspace:true,\r\n    backDelay: 1500,\r\n    loop:true,\r\n    loopCount:false,\r\n    showCursor: true,\r\n    cursorChar:\"|\",\r\n    contentType:'html',\r\n})\r\n\r\n//Botones \r\nconst btn = document.querySelector('.btn');\r\nbtn.onmousemove = function(e){\r\n    const x = e.pageX - btn.offsetLeft;\r\n    const y = e.pageY - btn.offsetTop;\r\n\r\n    btn.style.setPorperty('--x', x +'px')\r\n    btn.style.setPorperty('--y', y +'px')\r\n}"},"lineCount":null}},"error":null,"hash":"74922326b54df7389ef8639ab7661ba1","cacheData":{"env":{}}}